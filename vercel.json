{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_BASE_URL": "http://localhost:4000",
    "REACT_APP_HEARTBEAT_INTERVAL": "600000",
    "REACT_APP_API_TIMEOUT": "10000",
    "REACT_APP_API_RETRY_ATTEMPTS": "3",
    "REACT_APP_APP_NAME": "WorkSens Employee Client",
    "REACT_APP_VERSION": "1.0.0",
    "REACT_APP_COMPANY_NAME": "Bhishma Solutions",
    "REACT_APP_LOG_LEVEL": "info",
    "REACT_APP_LOG_MAX_SIZE": "10485760",
    "REACT_APP_LOG_MAX_FILES": "5",
    "REACT_APP_ENABLE_DEV_TOOLS": "false",
    "REACT_APP_ENABLE_AUTO_UPDATER": "true",
    "REACT_APP_ENABLE_CRASH_REPORTING": "true",
    "REACT_APP_ENFORCE_HTTPS": "true",
    "REACT_APP_CERTIFICATE_VALIDATION": "true",
    "REACT_APP_LOCATION_TIMEOUT": "15000",
    "REACT_APP_LOCATION_HIGH_ACCURACY": "true",
    "REACT_APP_LOCATION_MAX_AGE": "60000",
    "REACT_APP_HEARTBEAT_MAX_FAILURES": "3",
    "REACT_APP_THEME": "default",
    "REACT_APP_MINIMIZE_TO_TRAY": "true",
    "REACT_APP_AUTO_START": "false",
    "GENERATE_SOURCEMAP": "false"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Permissions-Policy": "geolocation=(self)"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "silent": true
  }
}
